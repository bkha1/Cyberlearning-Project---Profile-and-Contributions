<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee                http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <!-- Servlets -->
  <servlet>
    <servlet-name>contributionService</servlet-name>
    <servlet-class>cyber.learning.project.server.providers.ContributionServiceProvider</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>contributionService</servlet-name>
    <url-pattern>/cyberlearning/contribution</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>storeService</servlet-name>
    <servlet-class>cyber.learning.project.server.StoreServiceImpl</servlet-class>
  </servlet> 
  <servlet-mapping>
    <servlet-name>storeService</servlet-name>
    <url-pattern>/cyberlearning/store</url-pattern>
  </servlet-mapping>


  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>CyberLearning.html</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value/>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>

</web-app>